https://download.gnome.org/sources/libnma/1.10/libnma-1.10.6.tar.xz


Configure --disable-mobile-broadband-provider-info --with-libnma-gtk4=yes --enable-vala=no

meson --prefix=/usr --buildtype=release -Dgtk_doc=false -Dlibnma_gtk4=true  -Dintrospection=false -Dvapi=false -Dmobile_broadband_provider_info=false .. &&